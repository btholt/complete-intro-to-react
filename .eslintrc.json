{
  "extends": ["plugin:flowtype/recommended", "airbnb", "prettier", "prettier/react"],
  "plugins": ["flowtype", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  }
}
