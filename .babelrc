{
	"presets": [
		"react",
		["env", {
			"targets": {
				"browsers": "last 2 versions"
			},
			"loose": true,
			"modules": false
		}]
	],
  "plugins": [
    "babel-plugin-transform-class-properties"
  ],
	"env": {
		"test": {
			"plugins": [
				"transform-es2015-modules-commonjs"
			]
		}
	}
}
